/**app.wxss**/
page {
  background: #f4f6fb;
  color: #2b2f36;
  font-family: -apple-system, BlinkMacSystemFont, "Helvetica Neue", "PingFang SC", "Microsoft YaHei", sans-serif;
}

.page {
  min-height: 100vh;
  display: flex;
  flex-direction: column;
}

.content {
  flex: 1;
  padding-bottom: calc(120rpx + env(safe-area-inset-bottom));
}
